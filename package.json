{
  "//": "node version v20.11.0, 해당 버젼 esm 적용 버그가 있어 아래 스크립트와 같이 사용해야함",
  "name": "cheonil-restaurant-node",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node --no-warnings=ExperimentalWarning --loader ts-node/esm ./src/index.ts",
    "dev": "nodemon src/index.ts",
    "model": "sequelize-auto -o './src/models' --caseFile p --caseModel p -d cheonildb -h localhost -u root -x root -p 3306 -e mysql -l ts && eslint --fix ./src/models",
    "gm": "tsx ./generate-models.ts && eslint --fix ./src/models" 
  },
  "type": "module",
  "dependencies": {
    "@rushstack/eslint-patch": "^1.7.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "mysql2": "^3.6.5",
    "sequelize": "^6.35.2"
  },
  "devDependencies": {
    "@tsconfig/node16": "^16.1.1",
    "@types/cookie-parser": "^1.4.6",
    "@types/debug": "^4.1.12",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "nodemon": "^3.0.2",
    "prettier": "3.1.1",
    "sequelize-auto": "^0.8.8",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
