{
    "env": {
        "node": true,
        "commonjs": true,
        "es2021": true
    },
    "extends":  ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint"],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-unused-vars": "warn"
    },
    "root": true
}
